<%= form_with model:book,local:true do |f| %>
  <div class="form-group">
    <%= f.label :Title %></br>
    <%= f.text_field :title %>
  </div>
  <div class="form-group">
  	<%= f.label :Opinion %></br>
    <%= f.text_area :body %>
  </div>
    <%= f.submit 'Create Book',class: 'btn btn-success' %>
<% end %>